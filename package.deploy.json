{
  "name": "ide-backend",
  "version": "1.0.0",
  "description": "IDE Backend with node-pty terminal support",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node startup.js && node server.js",
    "dev": "nodemon server.js",
    "startup": "node startup.js",
    "test": "node test_terminal.js",
    "build": "npm install --production --no-optional",
    "health": "curl -s http://localhost:8000/health | jq . || echo 'Health check failed'",
    "sessions": "curl -s http://localhost:8000/terminal/sessions | jq . || echo 'Sessions check failed'"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "node-pty": "^1.0.0",
    "multer": "^1.4.5-lts.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
